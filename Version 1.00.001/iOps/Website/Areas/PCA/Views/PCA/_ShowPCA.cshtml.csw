<script>
    function get_value(tag) {
        return OPC.get_value(tag);

        //return OPC.get_value(OPC_config.tag_prefix + tag);
    }
    function get_bool(tag) {
        var b = OPC.get_value(tag);
        //var b = OPC.get_value(OPC_config.tag_prefix + tag);
        if (b && b == "True") return true;
        if (b && b == "False") return false;
        return null;
    }

    function createGauges() {
        $("#gaugeTEMP").kendoRadialGauge({
            pointer: [{
                value: 0,
                color: "#009900",
                cap: { size: 0.15 }
            }, {
                value: 0,
                color: "#ff7a00",
                cap: { size: 0.1 }
            }, {
                value: 0,
                color: "#66aaf7",
            }],
            scale: {
                minorUnit: 5,
                startAngle: -30,
                endAngle: 210,
                max: 140,
                labels: {
                    position: "outside"
                },
                ranges: [
                    {
                        from: 0,
                        to: 35,
                        color: "#c20000"
                    }, {
                        from: 35,
                        to: 65,
                        color: "#ffd400"
                    }, {
                        from: 65,
                        to: 90,
                        color: "#00c000"
                    }, {
                        from: 90,
                        to: 105,
                        color: "#ffd400"
                    }, {
                        from: 105,
                        to: 140,
                        color: "#c20000"
                    }
                ]
            }
        })
        $("#gaugePRESSSUC").kendoRadialGauge({
            pointer: [{
                value: 0,
                color: "#009900",
                cap: { size: 0.15 }
            }, {
                value: 0,
                color: "#ff7a00",
                cap: { size: 0.1 }
            }, {
                value: 0,
                color: "#ffc700",
            }],
            scale: {
                minorUnit: 5,
                startAngle: -30,
                endAngle: 210,
                max: 180,
                labels: {
                    position: "outside"
                },
                ranges: [
                    {
                        from: 80,
                        to: 120,
                        color: "#00c000"
                    }, {
                        from: 120,
                        to: 150,
                        color: "#ffd400"
                    }, {
                        from: 150,
                        to: 180,
                        color: "#c20000"
                    }
                ]
            }
        })
        $("#gaugePRESSHEAD").kendoRadialGauge({
            pointer: [{
                value: 0,
                color: "#009900",
                cap: { size: 0.15 }
            }, {
                value: 0,
                color: "#ff7a00",
                cap: { size: 0.1 }
            }, {
                value: 0,
                color: "#ffc700",
            }],
            scale: {
                minorUnit: 5,
                startAngle: -30,
                endAngle: 210,
                max: 180,
                labels: {
                    position: "outside"
                },
                ranges: [
                    {
                        from: 80,
                        to: 120,
                        color: "#00c000"
                    }, {
                        from: 120,
                        to: 150,
                        color: "#ffd400"
                    }, {
                        from: 150,
                        to: 180,
                        color: "#c20000"
                    }
                ]

            }
        })
    }

    function opc_callback(data) {
        Update_Tagged_Controls();
    }

    function Update_Tagged_Controls() {
        var pre = OPC_config.tag_prefix;
        var gauge = $("#gaugeTEMP").data("kendoRadialGauge");
        gauge.pointers[0].value(parseInt(get_value(pre + "TEMPDISCH.Value")));
        gauge.pointers[1].value(parseInt(get_value(pre + "TEMPAMB.Value")));
        gauge.pointers[2].value(parseInt(get_value(pre + "TEMPCABIN.Value")));

        gauge = $("#gaugePRESSSUC").data("kendoRadialGauge");
        gauge.pointers[0].value(parseInt(get_value(pre + "PRESSSUCPRI1.Value")));
        gauge.pointers[1].value(parseInt(get_value(pre + "PRESSSUCSEC1.Value")));
        gauge.pointers[2].value(parseInt(get_value(pre + "PRESSSUCSEC2.Value")));

        gauge = $("#gaugePRESSHEAD").data("kendoRadialGauge");
        gauge.pointers[0].value(parseInt(get_value(pre + "PRESSHEADPRI1.Value")));
        gauge.pointers[1].value(parseInt(get_value(pre + "PRESSHEADSEC1.Value")));
        gauge.pointers[2].value(parseInt(get_value(pre + "PRESSHEADSEC2.Value")));
    }

    var tags = [];
    $(document).ready(function () {
        OPC_config.Token = "7e61b230-481d-4551-b24b-ba9046e3d8f2";
        OPC_config.ServerURL = "http://10.28.100.3:58725";
        //OPC_config.tag_prefix = 'Airport-JFK.Airport.' + @TempData["ClientAbbr"] + '.Term4.Zone' + @TempData["ZoneNumber"] + '.Gate-' + @TempData["GateNumber"] + '.PCA.';
        @*OPC_config.tag_prefix = '\\\\opc.iopsnow.com\\RemoteSCADAHosting.Airport-' + '@TempData["ClientAbbr"]' + '.Airport.CID.Term1.Zone' + '@TempData["ZoneNumber"]' + '.GateC' + '@TempData["GateNumber"]' + '.PCA.';*@
