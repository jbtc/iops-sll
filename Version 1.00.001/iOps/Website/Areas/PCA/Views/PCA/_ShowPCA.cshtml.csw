                CloseKendoPCA();
                $('#main-body').html(data);
            }
        });
    });

    $('#PCAAlarmWindow').kendoWindow({
        width: '400px',
        height: '650px',
        animation: false,
        modal: false,
        draggable: true,
        title: 'Gate 1 - PCA Alarms',
